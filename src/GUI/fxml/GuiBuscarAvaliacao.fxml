<AnchorPane fx:id="pane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.controles.ControleBuscarAvaliacao">
   <children>
      <ImageView fitHeight="600.0" fitWidth="794.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../telasGUI/buscarAvaliacao.png" />
