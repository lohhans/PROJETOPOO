      </ImageView>
      <Label fx:id="labelCpf" layoutX="129.0" layoutY="142.0" prefHeight="19.0" prefWidth="42.0" text="CPF:">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <TextField fx:id="cpf" layoutX="260.0" layoutY="138.0" prefHeight="26.0" prefWidth="388.0" promptText="Apenas números (11 dígitos)" />
      <Button fx:id="botaoBuscar" layoutX="340.0" layoutY="210.0" mnemonicParsing="false" onAction="#buscarAvaliacao" text="Realizar busca" textAlignment="CENTER">
