      <Button fx:id="botaoPagarMensalidade" layoutX="485.0" layoutY="341.0" mnemonicParsing="false" onAction="#acaoChamarTelaPagarMensalidade" prefHeight="29.0" prefWidth="199.0" text="Pagar mensalidade">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <Button fx:id="botaoVoltar" layoutX="20.0" layoutY="560.0" mnemonicParsing="false" onAction="#acaoBotaoVoltar" text="Sair">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
